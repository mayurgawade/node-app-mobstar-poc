    "@types/jsonwebtoken": "^7.2.8",
    "@types/lodash": "^4.14.116",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^3.1.4",
    "concurrently": "^3.6.1",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "ts-node": "^7.0.1",
